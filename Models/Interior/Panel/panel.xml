<PropertyList>

    <path>panel.ac</path>

	<!-- Backlighting system -->
  <animation>
    <name>BackLightAnimation</name>
    <type>light</type>
    <light-type>spot</light-type>
    <object-name>backlight</object-name>
    <position>
      <x> 0.035 </x>
      <y> 0.01 </y>
      <z> -0.05 </z>
    </position>
    <direction>
      <x> -1.0 </x>
      <y>  0.0 </y>
      <z>  1.0 </z>
    </direction>
    <ambient>
      <r>  0.0 </r>
      <g>  0.0 </g>
      <b>  0.0 </b>
      <a>  1.0 </a>
    </ambient>
    <diffuse>
      <r>  0.5 </r>
      <g>  0.404 </g>
      <b>  0.2 </b>
      <a>  1.0 </a>
    </diffuse>
    <specular>
      <r>  0.0 </r>
      <g>  0.0 </g>
      <b>  0.0 </b>
      <a>  1.0 </a>
    </specular>
    <dim-factor>
      <property>systems/electrical/outputs/instrument-lights</property>
      <!-- optional begin -->
      <factor>50</factor>
      <offset>0</offset>
      <min>0</min>
      <max>1</max>
      <!-- optional end -->
    </dim-factor>
    <attenuation>
      <c>  0.2 </c>
      <l>  0.2 </l>
      <q>  0.4 </q>
    </attenuation>
    <exponent>  8.0 </exponent>
    <cutoff>   48.0   </cutoff>
    <near-m>    0.0   </near-m>
    <far-m>     0.09 </far-m>
  </animation>
  <animation>
    <type>select</type>
    <object-name>BackLightAnimation</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
  </animation>

